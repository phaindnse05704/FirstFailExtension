{
    "manifest_version": 2,
    "name": "Pomodoro Timer",
    "version": "1.0",
    "minimum_chrome_version": "23",
    "browser_action": {
        "default_icon": "icon_16.png",
        "default_popup": "index.html",
        "default_title": "Pomodoro Timer"
    },
    "icons": {
        "16": "icon_16.png",
        "128": "icon_128.png"
    },
    "background": {
        "scripts": ["js/jquery-3.3.1.min.js", "script.js"],
        "persistent": true
    },
    "permissions": [
        "activeTab"
    ],
    "content_security_policy": "script-src 'self' blob: filesystem: chrome-extension-resource: ; object-src 'self'"
}